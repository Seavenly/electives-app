{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": [true, "array"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}

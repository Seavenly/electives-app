{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": [true, "array"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "import-name": false,
    "no-unused-variable": true,
    "no-unused-expression": true
  }
}
